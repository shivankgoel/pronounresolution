     <ENAMEX TYPE="PERSON">Leighton E. Cluff</ENAMEX> <ENAMEX TYPE="PER_DESC">M.D.</ENAMEX> <ENAMEX TYPE="PER_DESC">President</ENAMEX> <ENAMEX TYPE="ORGANIZATION:OTHER">Robert Wood Johnson Foundation</ENAMEX> <ENAMEX TYPE="GPE:CITY">Princeton</ENAMEX> , <ENAMEX TYPE="GPE:STATE_PROVINCE">N.J .</ENAMEX>
